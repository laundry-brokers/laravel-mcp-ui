{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "mcp-ui/laravel-mcp-ui",
    "description": "MCP-UI SDK for Laravel 12 and FilamentPHP 4",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "mcp",
        "mcp-ui",
        "filament",
        "ai"
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "filament/forms": "4.0"
    },
    "autoload": {
        "psr-4": {
            "McpUi\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "McpUi\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "McpUi\\Laravel\\McpUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
